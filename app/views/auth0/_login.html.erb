<script src="https://cdn.auth0.com/js/lock-7.1.min.js"></script>
<script type="text/javascript">

var lock = new Auth0Lock('ccTVQCHKxUJRTc97wjgJtQOccbHSFAHf', 'nightfire.auth0.com');


function signin() {
    lock.show({
            callbackURL: '<%= AUTH0_CALLBACK_URL %>'
            , responseType: 'code'
            , authParams: {
                scope: 'openid profile'
            }
    });
}
</script>
<a class="sign-up" onclick="signin()">Login</a>
